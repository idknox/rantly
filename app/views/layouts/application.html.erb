<!DOCTYPE html>
<html>
<head>
  <title>Rantly</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="nav">
  <div class="title"></div>
  <div class="nav_buttons">
    <%= link_to "Login", signin_path %>
    <%= link_to "Join", new_user_path %>
  </div>
</div>
<% if flash[:notice] %>
  <div class="flash"><%= flash[:notice] %></div>
<% end %>
<%= yield %>

</body>
</html>
